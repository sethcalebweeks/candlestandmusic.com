---
import Layout from '../layouts/Layout.astro';
---

<Layout>
	<main>
    <video autoplay muted loop id="myVideo">
      <source src="sailing.mp4" type="video/mp4">
    </video>
	</main>
</Layout>

<style>
	main {
    width: 100%;
    height: 100%;
  }
  video {
    width: 100%;
    height: 100%;
    object-fit: cover;
    object-position: 45% 90%;
  }
</style>
